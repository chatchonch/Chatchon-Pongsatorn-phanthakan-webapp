var canvas = document.getElementById('myCanvas');
var context = canvas.getContext('2d');
context.canvas.width  = window.innerWidth;
context.canvas.height = window.innerHeight;

canvas.addEventListener('mousemove', function(evt) {
    spotlight(canvas, getMousePos(canvas, evt));
}, false);

function spotlight(canvas, coord) {
    var context = canvas.getContext('2d');
    context.clearRect(0,0,canvas.width, canvas.height);
    context.fillRect(0,0,window.innerWidth,window.innerHeight);
    context.font = "14px Arial";
    context.fillText("There's nothing here",width(10),height(30));
    context.fillText("what are you looking for?",width(45),height(60));
    context.fillText("Nothing!",width(80),height(20));
    context.fillText("It's very dark in here",width(75),height(64));
    context.fillText("I am blind help!",width(60),height(10));
    context.fillText("That look like a flag",width(10),height(90));
    context.fillText("I'm just kidding",width(30),height(80));
    context.fillText("Try more to gain more",width(80),height(95));
    var grd = context.createRadialGradient(
        coord.x, coord.y,  50,
        coord.x, coord.y,  30);
    grd.addColorStop(0,'rgba(255,255,255,0)');
    grd.addColorStop(1,'rgba(255,255,255,.7)');
    context.fillStyle=grd;
}
function getMousePos(canvas, evt) {
    var rect = canvas.getBoundingClientRect();
    return {
        x:  evt.clientX - rect.left,
        y:  evt.clientY - rect.top
    };
}
function height(perc)
{
    var h = window.innerHeight;
    return h * (perc / 100);
}
function width(perc)
{
    var w = window.innerWidth;
    return w * (perc / 100);
}

